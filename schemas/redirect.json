{
  "fields": [
    {
      "protocols": {
        "required": true,
        "type": "set",
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "elements": {
          "type": "string",
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ]
        },
        "description": "A set of strings representing HTTP protocols."
      }
    },
    {
      "config": {
        "fields": [
          {
            "status_code": {
              "required": true,
              "type": "integer",
              "between": [
                100,
                599
              ],
              "default": 301,
              "description": "The response code to send. Must be an integer between 100 and 599."
            }
          },
          {
            "location": {
              "description": "The URL to redirect to",
              "type": "string",
              "required": true
            }
          },
          {
            "keep_incoming_path": {
              "description": "Use the incoming request's path and query string in the redirect URL",
              "default": false,
              "type": "boolean"
            }
          }
        ],
        "type": "record",
        "required": true
      }
    }
  ],
  "entity_checks": []
}