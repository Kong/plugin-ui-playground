{
  "fields": [
    {
      "protocols": {
        "type": "set",
        "description": "A set of strings representing HTTP protocols.",
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ],
          "type": "string"
        },
        "required": true
      }
    },
    {
      "config": {
        "fields": [
          {
            "status_code": {
              "type": "integer",
              "description": "The response code to send. Must be an integer between 100 and 599.",
              "default": 301,
              "between": [
                100,
                599
              ],
              "required": true
            }
          },
          {
            "location": {
              "type": "string",
              "description": "The URL to redirect to",
              "required": true
            }
          },
          {
            "keep_incoming_path": {
              "type": "boolean",
              "description": "Use the incoming request's path and query string in the redirect URL",
              "default": false
            }
          }
        ],
        "required": true,
        "type": "record"
      }
    }
  ],
  "entity_checks": []
}